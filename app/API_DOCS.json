openapi: 3.0.1
info:
  title: Myrent Booking API
  description: booking API for reference
  version: mock-api
servers:
  - url: 'https://virtserver.swaggerhub.com/myrent/booking/mock-api'
  - url: 'https://demo.myrent.it/MyRentDemoEN'
  - url: 'https://se.mercedes.myrent.it/MyRentMoveMee'
paths:
  /api/v1/touroperator/authentication:
    post:
      summary:  Authentication details
      description: Get Authentication token for other request
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                UserId:
                  type: string
                Password:
                  type: string
                CompanyCode:
                  type: string
            examples:
              '0':
                value: "{\r\n    \"UserId\":\"xyz\",\r\n    \"Password\":\"XXXXXXX\",\r\n    \"companyCode\":\"*******\"\r\n}"
      responses:
        '200':
          description: OK
          content:
            application/json:
              schema:
                 type: object
              examples:
                '0':
                  value: "{\r\n    \"status\": true,\r\n    \"message\": \"success\",\r\n    \"result\": {\r\n        \"user_id\": 1,\r\n        \"username\": \"xyz\",\r\n        \"tokenValue\": \"63b2960ffc4c38fbdbeec8\",\r\n        \"userRole\": \"Administrator\"\r\n    }\r\n}"

  /api/v1/touroperator/locations:
    get:
      summary: find locations
      description:  <b>Q.2-</b> locations - what are the possible type values? <br>
        <b>Location Type</b>- My Rent to provide the possible values</br>
        BOOKING_PICKUP = 1 (location allowed only for pick up)</br>
        BOOKING_DROPOFF = 2 (location allowed only for dropoff)</br>
        BOOKING_BOTH = 3 (location allowed for pick up / drop off)</br>
         <b>Q3-</b> locations - Is the location code an IATA code? if so, do you have one for all locations?
         <br>
        <b>Location Code</b>-Itâ€™s instance specific, but will be IATA if one is available.  </br>
        [QUESTION ] can they change these values?<br>
        [ANSWER] Yes the customer can change the value


      parameters:
        - name: tokenValue
          in: header
          schema:
            type: string
          example: a2f47f7d727cb7c3a9d7321cc69e6004
      responses:
        '200':
          description: Auto generated using Swagger Inspector
          content:
            application/json:
              schema:
                  $ref: '#/components/schemas/locationResponse'
              examples:
                '0':

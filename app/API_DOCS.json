  /api/v1/touroperator/damages/{plateNumber/vin}:
    get:
      summary: find damages
      description:  vehicle damages with wireframe image
      parameters:
        - name: tokenValue
          in: header
          schema:
            type: string
          example: a2f47f7d727cb7c3a9d7321cc69e6004
        - name: Accept-Language
          in: header
          schema:
            type: string
          example: en_GB
        - name: plateNumber/vin
          in: path
          required: true
          schema:
            type: string
            example: 'FT233AP'
      responses:
        '200':
          description: get vehicle damages with wireframe image
          content:
            application/json:
              schema:
                type: object
                properties:
                  data:
                    type: object
                    properties:
                      damages:
                        type: array
                        items:
                          $ref: '#/components/schemas/Damage'
                      wireframeImage:
                        type: object
                        items:
                          $ref: '#/components/schemas/wireframeImage'
